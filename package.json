    "@tsconfig/node18": "^18.2.2",
    "@types/js-cookie": "^3.0.4",
    "@types/nightwatch": "^2.3.25",
    "@types/node": "^18.17.17",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
